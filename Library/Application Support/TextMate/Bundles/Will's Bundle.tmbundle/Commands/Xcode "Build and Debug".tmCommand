<dict>
	<key>beforeRunningCommand</key>
	<string>saveModifiedFiles</string>
	<key>command</key>
	<string>#!/usr/bin/env python
import os
CMD="""
ignoring application responses
	tell application "Xcode" to build
end ignoring
ignoring application responses
	tell application "Xcode" to debug
end ignoring
"""
print os.popen("osascript -e '"+CMD+"'").read()
</string>
	<key>input</key>
	<string>selection</string>
	<key>keyEquivalent</key>
	<string>@y</string>
	<key>name</key>
	<string>Xcode "Build and Debug"</string>
	<key>output</key>
	<string>showAsTooltip</string>
	<key>scope</key>
	<string>source.c, source.objc, source.c++, source.objc++</string>
	<key>uuid</key>
